{
  "name": "@hitomihiumi/micro-docgen",
  "version": "0.4.0",
  "description": "Quick and easy documentation for typescript projects.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "docgen": "./bin/index.js",
    "micro-docgen": "./bin/index.js"
  },
  "scripts": {
    "format": "prettier --write \"./{src,examples,docs,scripts}/**/*.{js,ts,md,mdx}\"",
    "docs": "tsx ./scripts/generate.ts",
    "example:md": "cd ./examples/markdown && tsx generator.ts",
    "example:json": "cd ./examples/awesome-widgets && tsx generator.ts",
    "build": "tsc",
    "prepublishOnly": "pnpm build && pnpm docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NMMTY/LazyCanvas.git"
  },
  "keywords": [
    "docgen",
    "docs",
    "jsdoc",
    "typescript",
    "documentation",
    "api",
    "json",
    "typedoc"
  ],
  "author": "hitomihiumi, twlite <hello@twlite.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NMMTY/LazyCanvas/issues"
  },
  "homepage": "https://github.com/NMMTY/LazyCanvas/tree/main/packages/micro-docgen#readme",
  "dependencies": {
    "@hitomihiumi/micro-docgen": "link:",
    "commander": "^12.1.0",
    "common-tags": "^1.8.2",
    "tmp": "^0.2.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1",
    "@types/node": "^20.10.2",
    "@types/tmp": "^0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.2"
  }
}
